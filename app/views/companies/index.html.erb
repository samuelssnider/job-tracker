<h3>All Companies</h3>
<p><%= link_to "Create New Company", new_company_path %></p>
<ul>
  <% @companies.each do |company| %>
  <li class=company_<%= company.id %>>
    <%= link_to company.name, company_path(company) %> <br />
    <%= link_to "Edit", edit_company_path(company) %> <br />
    <%= link_to "Delete", company_path(company), method: "delete" %>
  </li>
  <% end %>
</ul>
